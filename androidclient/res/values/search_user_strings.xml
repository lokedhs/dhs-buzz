<resources>
    <!-- Window title of the user search window -->
    <string name="search_user_window_title">User search</string>

    <!-- Text displayed above the user list after searching for a user. Processed by
     MessageFormat. {0} is the number of persons found. -->
    <string name="search_user_result_description">Found {0,number} {0,choice,0#persons|1#person|1&lt;persons}</string>

    <!-- Label in the button that starts a user search -->
    <string name="search_user_begin">Go</string>

    <!-- Name of the option "search user by name" in the type menu -->
    <string name="search_user_type_name">Name</string>

    <!-- Name of the option "search user by topic" in the type menu -->
    <string name="search_user_type_topic">Topic</string>

    <!-- The hint text in the search text box -->
    <string name="search_user_text_view_title">Type search query</string>

    <!-- Text in the user list for the row that allows the user to load more entries -->
    <string name="search_user_load">Load more&#8230;</string>

    <!-- Error message that is displayed in a toast if the user pressed the search button while a search is already in progress -->
    <string name="search_user_already_in_progress">Search already in progress</string>

    <!-- Error message displayed in a toast if there was an error loading the users. {0} - the error message -->
    <string name="search_user_error_loading_users">Error loading user list: {0}</string>
</resources>
